{
  "nyc": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "scripts": {
    "dev": "node ./app.js",
    "test-mocha": "mocha",
    "mocha": "nyc --reporter=html --reporter=text mocha",
    "test": "cross-env NODE_ENV=test nyc mocha --exit"
  },
  "dependencies": {
    "busboy": "^0.2.14",
    "co-wechat-oauth": "^2.0.1",
    "cos-nodejs-sdk-v5": "^2.0.12",
    "ejsexcel": "^3.5.2",
    "eslint-plugin-import": "^2.8.0",
    "fanqier-wxredpack": "^1.0.0",
    "ioredis": "^3.1.4",
    "ioredis-lock": "^3.4.0",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-jwt": "^3.3.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "mongoose": "^4.11.12",
    "node-xlsx": "^0.11.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "ioredis-lock": "^3.4.0",
    "istanbul": "^1.1.0-alpha.1",
    "koa-jwt": "^3.3.0",
    "log4js": "^2.3.12",
    "mocha": "^4.0.1",
    "mochawesome": "^3.0.0",
    "moment": "^2.20.0",
    "mysql2": "^1.5.1",
    "nyc": "^11.4.1",
    "sequelize": "^4.25.1",
    "source-map-support": "^0.5.0",
    "supertest": "^3.0.0"
  }
}
